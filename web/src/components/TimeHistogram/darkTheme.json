        "g2-tooltip-marker": {
          "width": "8px",
          "height": "8px",
          "borderRadius": "50%",
          "display": "inline-block",
          "marginRight": "8px"
        },
        "g2-tooltip-value": {
          "display": "inline-block",
          "float": "right",
          "marginLeft": "30px"
        }
