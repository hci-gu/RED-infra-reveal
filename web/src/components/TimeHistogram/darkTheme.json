          "alignTick": true,
          "animate": true
        }
      },
      "radius": {
        "title": null,
        "grid": {
          "line": {
            "type": "circle",
            "style": { "stroke": "#262626", "lineWidth": 1, "lineDash": null }
          },
          "alignTick": true,
          "animate": true
        }
      }
