      "top": {
        "position": "top",
        "grid": null,
        "title": null,
        "verticalLimitLength": 0.5
      },
      "bottom": {
        "position": "bottom",
        "grid": null,
        "title": null,
        "verticalLimitLength": 0.5
      },
      "left": {
        "position": "left",
        "title": null,
        "line": null,
        "tickLine": null,
        "verticalLimitLength": 0.3333333333333333
